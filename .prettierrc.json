{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "semi": true,
  "useTabs": true,
  "bracketSameLine": true,
  "endOfLine": "auto",
  "importOrder": [
    "^@(.*)$",
    "^((\\.\\.\\/((\\.|\\/)*))|~\\/|\\.\\.)((?!assets)(?!styles).)*$",
    "^\\.\\/((?!assets)(?!styles).)*\\/(.*)$",
    "^\\.((?!assets)(?!styles).)*$",
    "^((\\.\\.\\/((\\.|\\/)*))|~\\/)(?=assets)(.*)$",
    "^\\.\\/assets(.*)$",
    "^((\\.\\.\\/((\\.|\\/)*))|~\\/)(.*)(?=styles)(.*)$",
    "^\\.\\/(.*)\\.styles(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
